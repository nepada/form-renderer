{
    "name": "nepada/form-renderer",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Latte template based form renderer for Nette forms with full support for Bootstrap 3, 4 & 5.",
    "keywords": ["forms", "renderer", "bootstrap", "nette", "latte"],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/form-renderer/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=8.1.0 <8.5",
        "nette/utils": "^3.2@dev || ^4.0@dev",
        "nette/forms": "^3.1@dev",
        "nette/application": "^3.1.13@dev",
        "latte/latte": "^3.0@dev"
    },
    "require-dev": {
        "nette/tester": "v2.5.5",
        "mockery/mockery": "1.6.12",
        "phpstan/phpstan": "2.1.17",
        "phpstan/phpstan-strict-rules": "2.0.4",
        "phpstan/phpstan-nette": "2.0.4",
        "nepada/phpstan-nette-tester": "2.0.0",
        "spaze/phpstan-disallowed-calls": "4.5.0",
        "shipmonk/phpstan-rules": "4.1.4",
        "phpstan/phpstan-mockery": "2.0.0",
        "php-parallel-lint/php-parallel-lint": "1.4.0",
        "nepada/coding-standard": "7.15.1",
        "nette/di": "^3.0.6@dev",
        "nette/bootstrap": ">=3.1@dev",
        "nette/component-model": ">=3.0.2",
        "composer-runtime-api": "^2.0",
        "composer/semver": "3.4.4",
        "nette/http": ">=3.1@dev",
        "nette/neon": ">=3.3.4@dev",
        "nette/robot-loader": "*@dev"
    },
    "suggest": {
        "nette/di": "for integration with Nette DI container"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\Bridges\\": "src/Bridges/",
            "Nepada\\FormRenderer\\": "src/FormRenderer/"
        },
        "files": ["src/compatibility.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\": "tests/"
        }
    },
    "config": {
        "platform-check": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.12-dev"
        }
    }
}
