{
    "name": "nepada/form-renderer",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Latte template based form renderer for Nette forms with full support for Bootstrap 3 & 4.",
    "keywords": ["forms", "renderer", "bootstrap", "nette", "latte"],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/form-renderer/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "nette/utils": "^3.1.3@dev",
        "nette/forms": "^3.0@dev",
        "nette/application": "^3.0.7@dev",
        "latte/latte": "^2.6.0@dev"
    },
    "require-dev": {
        "nette/tester": "2.4.0",
        "mockery/mockery": "1.4.3",
        "phpstan/phpstan": "0.12.82",
        "phpstan/phpstan-strict-rules": "0.12.9",
        "pepakriz/phpstan-exception-rules": "0.11.6",
        "phpstan/phpstan-nette": "0.12.16",
        "nepada/phpstan-nette-tester": "0.3.1",
        "spaze/phpstan-disallowed-calls": "1.4.0",
        "phpstan/phpstan-mockery": "0.12.13",
        "php-parallel-lint/php-parallel-lint": "1.2",
        "nepada/coding-standard": "7.3",
        "nette/bootstrap": "^3.0@dev",
        "nette/di": "^3.0.5@dev",
        "nette/http": "^3.0@dev",
        "nette/caching": "^3.0@dev"
    },
    "suggest": {
        "nette/di": "for integration with Nette DI container"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\Bridges\\": "src/Bridges/",
            "Nepada\\FormRenderer\\": "src/FormRenderer/"
        },
        "files": ["src/compatibility.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\": "tests/"
        }
    },
    "config": {
        "platform-check": false
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.7-dev"
        }
    }
}
